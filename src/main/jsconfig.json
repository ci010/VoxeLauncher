{
    "compilerOptions": {
        "target": "es5",
        "strict": true,
        "baseUrl": ".",
        "checkJs": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "universal/*": [
                "../universal/*"
            ],
            "static/*": [
                "../../static/*"
            ],
            "main/*": [
                "./*"
            ]
        },
    },
    "include": [
        ".",
        "./ipc.d.ts",
        "../../types/*"
    ]
}