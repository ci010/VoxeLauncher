{
    "compilerOptions": {
        "target": "es5",
        "strict": true,
        "baseUrl": ".",
        "checkJs": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "universal/*": [
                "./*"
            ],
            "static/*": [
                "../../static/*"
            ],
        },
    },
    "include": [
        ".",
        "../../types/*",
    ]
}